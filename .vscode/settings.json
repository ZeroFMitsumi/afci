{
    "intelephense.environment.phpVersion": "8.2.*",
    "php-docblocker.alignParams": true,
    "php-docblocker.alignReturn": true,
    "symfony-vscode.phpExecutablePath": "C:/.php/php",
    "php.validate.executablePath": "C:/.php/php",
    "php.executablePath": "C:/.php/php",
    "auto-close-tag.activationOnLanguage": [
        "*",
        "xml",
        "php",
        "blade",
        "ejs",
        "jinja",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "plaintext",
        "markdown",
        "vue",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml",
        "HTML (EEx)",
        "HTML (Eex)",
        "plist"
    ],
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.includeLanguages": {
        "twig": "html"
    },
    "symfony-vscode.enableFileWatching": false,
    "symfony-vscode.routesFilters": {
        "^_assetic_": "id"
    },
    "symfony-vscode.showConsoleErrors": false,
    "php-cs-fixer.autoFixBySemicolon": true,
    "php-cs-fixer.formatHtml": true,
    "php-cs-fixer.ignorePHPVersion": true,
    "discord.removeTimestamp": false,
    "symfony-vscode.phpPath": "C:/.php/php",
    "symfony-vscode.showAsseticRoutes": true,
    "symfony-vscode.detectCwd": true,
    "php.suggest.basic": true,
    "php.validate.enable": true,
    "php.format.rules.alignConsecutiveAssignments": false,
    "phpConstructor.chooseConstructorVisibility": true,
    "phpConstructor.choosePropertyVisibility": true,
    "php.debug.executablePath": "C:/.php/php",
    "php-docblocker.qualifyClassNames": true,
    "namespaceResolver.highlightOnSave": false,
    "namespaceResolver.highlightOnOpen": false,
    "namespaceResolver.sortOnSave": true,
    "phpfmt.cakephp": true,
    "phpfmt.detect_indent": true,
    "phpfmt.enable_auto_align": true,
    "phpfmt.php_bin": "C:/.php/php",
    "pretty-formatter.braceLine": true,
    "pretty-formatter.braces": true,
    "pretty-formatter.bracePadding": true,
    "pretty-formatter.compressedCss": true,
    "pretty-formatter.correct": true,
    "pretty-formatter.cssInsertLines": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "pretty-formatter.braceStyle": "expand",
    "pretty-formatter.endComma": false,
    "pretty-formatter.indentLevel": 4,
    "editor.defaultFormatter": "Codeium.codeium",
    "twig-language.compressedCss": true,
    "twig-language.braceStyle": "expand",
    "twig-language.indentLevel": 4,
    "twig-language-2.braceStyle": "expand",
    "twig-language-2.compressedCss": true,
    "twig-language-2.indentLevel": 4,
    "pretty-formatter.commentLine": true,
    "pretty-formatter.elseLine": true,
    "twig-language-2.braceLine": true,
    "twig-language-2.braces": true,
    "twig-language-2.bracePadding": true,
    "twig-language-2.commentLine": true,
    "twig-language-2.comments": false,
    "twig-language-2.correct": true,
    "twig-language-2.forceAttribute": true,
    "twig-language-2.forceIndent": true,
    "twigCodeSnippets.enableContribute": true,
    "editor.formatOnType": true,
    "diffEditor.experimental.collapseUnchangedRegions": true,
    "diffEditor.experimental.useVersion2": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
}